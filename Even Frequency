# C_Program

/* Accept N numbers from user and return frequency of even numbers.
Input : N : 6
Elements : 85 66 3 80 93 88
Output : 3

*/

#include<stdio.h>
#include<stdlib.h>

int FrqEvnNum(int *arr,int iValue )
{
	int i=0;
	int iCnt=0;

	for(i=0;i<iValue;i++)
	{
		if((arr[i]%2)==0)
		{
          iCnt++;
		}
	}
	return iCnt;
}

int main()
{
	int iValue=0,i=0,iRet=0;

	int *Arr=NULL;

	printf("How many elements");
	scanf("%d",&iValue);

	Arr=(int*)malloc(sizeof(int)*iValue);

	printf("Enter elements in the array");

	for(i=0;i<iValue;i++)
	{
		scanf("%d\t",&Arr[i]);
	}	

	iRet=FrqEvnNum( Arr,iValue);

	printf("frequency is:%d\n",iRet );

}
