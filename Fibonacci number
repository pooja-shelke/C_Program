# C_Program


#include <stdio.h>

void  Fibbonoic(int value)
{
    int i=0,c=0;
    int a=0,b=1;
    
    printf("%d\t",a);
     printf("%d\t",b);
    
    for(i=0;i<=value;i++)
    {
        c=a+b;
        printf("%d\t",c);
        a=b;
        b=c;
        
    }
    
}
    

int main() 
{
    int value=0;
    
    
    printf("How many number want in series");
    scanf("%d",&value);
    
    Fibbonoic(value);
    
    
    return 0;
}
